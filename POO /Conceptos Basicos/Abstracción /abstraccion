class Delivery():
    def __init__(self):
        self._status = 'is off'
    
    def on(self):
        self._status = 'is on'
        print('delivery is ready')
    
    def enCamino(self):
        if self._status == 'is off':
            self.on()
            print('delivery on')

# Creación de un objeto y llamada al método
deliver = Delivery()
deliver.enCamino()  
